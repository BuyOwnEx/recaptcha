{
    "name": "buyownex/recaptcha",
    "description": "A Laravel 5.5 package providing support for reCAPTCHA.",
    "homepage": "https://github.com/BuyOwnEx/recaptcha",
    "type": "library",
    "keywords": ["laravel", "package", "recaptcha"],
    "license": "MIT",
    "authors": [
        {
            "name": "BuyOwnEx Ltd.",
            "email": "support@buyownex.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "Ilya\\": "src/Ilya/"
        },
        "files": [
            "src/Ilya/Recaptcha/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ilya\\Recaptcha\\RecaptchaServiceProvider"
            ]
        }
    }
}
